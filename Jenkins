pipeline {
    agent any
 stages {
        stage('Clean') {
            steps {
               echo "project cleaning..."
              sh "mvn clean"
            }
        }
  
        stage('Build') {
            steps {
                 echo "project Building..."
                 sh "mvn compile"
            }
        }
        stage('Test') {
            steps {
                 echo "project Testing..."
                sh "mvn test"
            }
        }
        stage('Deploy') {
            steps {
                echo "project Deploying..."
              sh "mvn install"
            }
        }
    }
}
